{
  "name": "vue3-virtual-scroll",
  "version": "0.1.1",
  "main": "./dist/my-lib.umd.js",
  "module": "./dist/my-lib.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:npm": "cross-env TARGET=npm vite build && vue-tsc --declaration --emitDeclarationOnly && mv dist/lib dist/types",
    "build:page": "cross-env TARGET=page vite build",
    "serve": "vite preview",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.1",
    "vue": "^3.2.16",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefix": "^1.0.1",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vitest": "^0.6.0",
    "vue-tsc": "^0.33.2-patch.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "respository": {
    "type": "git",
    "url": "https://github.com/heikaimu/vue3-virtual-scroll"
  },
  "bugs": {
    "url": "https://github.com/heikaimu/vue3-virtual-scroll/issues"
  }
}
